        <record id="bt9" model="cl.reparation.newtest">
            <field name="name">Equipo de sustitución</field>
            <field name="type" ref="t1"/>
        </record>
        <!-- User test -->
        <record id="ut1" model="cl.reparation.newtest">
            <field name="name">Battery Test</field>
            <field name="type" ref="t2"/>
        </record>
        <record id="ut2" model="cl.reparation.newtest">
            <field name="name">Cámara #1 (1ª opción web: probar cámara)</field>
            <field name="type" ref="t2"/>
        </record>
        <record id="ut3" model="cl.reparation.newtest">
            <field name="name">Cámara #2</field>
            <field name="type" ref="t2"/>
        </record>
        <record id="ut4" model="cl.reparation.newtest">
            <field name="name">Micrófono (1ª opción web: probar micrófono)</field>
            <field name="type" ref="t2"/>
        </record>
        <record id="ut5" model="cl.reparation.newtest">
            <field name="name">Audio (videos YouTube etc)</field>
            <field name="type" ref="t2"/>
        </record>
    </data>
</odoo>