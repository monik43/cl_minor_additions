<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Test type -->
        <record id="t1" model="cl.test.type">
            <field name="name">Básico</field>
        </record>
        <record id="t2" model="cl.test.type">
            <field name="name">Usuario</field>
        </record>
        <!-- Basic test -->
        <record id="bt1" model="cl.reparation.newtest">
            <field name="name">WIFI</field>
            <field name="type" ref="t1"/>
        </record>
        <record id="bt2" model="cl.reparation.newtest">
            <field name="name">Teclado</field>
            <field name="type" ref="t1"/>
        </record>
        <record id="bt3" model="cl.reparation.newtest">
            <field name="name">Touchpad</field>
            <field name="type" ref="t1"/>
        </record>
        <record id="bt4" model="cl.reparation.newtest">
            <field name="name">Pantalla táctil (Si lo és)</field>
            <field name="type" ref="t1"/>
        </record>
        <record id="bt5" model="cl.reparation.newtest">
            <field name="name">Prueba carga (cargador original)</field>
            <field name="type" ref="t1"/>
        </record>
        <record id="bt6" model="cl.reparation.newtest">
            <field name="name">Prueba de carga (superior al 10%) 5% D 5% IZ</field>
            <field name="type" ref="t1"/>
        </record>
        <record id="bt7" model="cl.reparation.newtest">
            <field name="name">Tornillos</field>
            <field name="type" ref="t1"/>
        </record>
        <record id="bt8" model="cl.reparation.newtest">
            <field name="name">Modo tablet (Táctil y que funcione KB y TP)</field>
            <field name="type" ref="t1"/>
