<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="cl_helpdesk_ticket_view_form_misc_changes" model="ir.ui.view">
            <field name="name">cl_helpdesk.ticket.form.misc.changes</field>
            <field name="model">helpdesk.ticket</field>
            <field name="inherit_id" ref="helpdesk.helpdesk_ticket_view_form"/>
            <field name="arch" type="xml">
                <xpath expor="//field[@name='ordensat']" position="attributes">
                    <attribute name="context">{'default_lot_id': x_lot_id,'default_name': name_rma,'default_partner_id': partner_id, 'default_product_id': x_lot_it.product_id }</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>