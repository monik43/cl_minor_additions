<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="cl_stock_quant_form_add_CSN" model="ir.ui.view">
            <field name="name">cl_stock.quant.form.add.CSN</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='lot_id']" position="after">
                    <field name="CSN"/>
                </xpath>
            </field>
        </record>
        <record id="cl_stock_quant_tree_add_CSN" model="ir.ui.view">
            <field name="name">cl_stock.quant.tree.add.CSN</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='lot_id']" position="after">
                    <field name="CSN"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>